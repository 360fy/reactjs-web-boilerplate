{
  "name": "reactjs-web-boilerplate",
  "version": "1.1.1",
  "repository": "git@github.com:360fy/reactjs-web-boilerplate.git",
  "description": "Boilerplate for react based isomorphic application",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "agentkeepalive": "^2.0.5",
    "bluebird": "^3.3.4",
    "classnames": "^2.2.3",
    "events": "^1.1.0",
    "immutable": "^3.7.6",
    "jquery": "^2.2.2",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1",
    "rest": "^1.3.2",
    "velocity-animate": "^1.2.3",
    "velocity-react": "^1.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-node5": "^11.0.1",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.5.1",
    "eslint-config-360fy": "^1.1.1",
    "eslint-plugin-react": "^4.2.3"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
