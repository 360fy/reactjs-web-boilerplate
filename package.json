{
  "name": "reactjs-web-boilerplate",
  "version": "1.2.3",
  "repository": "git@github.com:360fy/reactjs-web-boilerplate.git",
  "description": "Boilerplate for react based isomorphic application",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.4.0",
    "classnames": "2.2.5",
    "events": "1.1.0",
    "history": "3.0.0",
    "immutable": "3.8.1",
    "jquery": "2.2.4",
    "lodash": "4.13.1",
    "react": "15.1.0",
    "react-addons-css-transition-group": "15.1.0",
    "react-dom": "15.1.0",
    "react-router": "2.4.1",
    "rest": "1.3.2",
    "velocity-animate": "1.2.3",
    "velocity-react": "1.1.5"
  },
  "devDependencies": {
    "babel-core": "6.9.1",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-node5": "11.1.0",
    "babel-preset-react": "6.5.0",
    "eslint": "2.11.1",
    "eslint-config-360fy": "1.1.9",
    "eslint-plugin-react": "5.1.1"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
