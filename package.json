{
  "name": "reactjs-web-boilerplate",
  "version": "1.1.3",
  "repository": "git@github.com:360fy/reactjs-web-boilerplate.git",
  "description": "Boilerplate for react based isomorphic application",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.5",
    "classnames": "^2.2.5",
    "events": "^1.1.0",
    "immutable": "^3.8.1",
    "jquery": "^2.2.3",
    "lodash": "^4.11.2",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "rest": "^1.3.2",
    "velocity-animate": "^1.2.3",
    "velocity-react": "^1.1.5"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-node5": "^11.0.2",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.8.0",
    "eslint-config-360fy": "^1.1.4",
    "eslint-plugin-react": "^4.3.0"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
