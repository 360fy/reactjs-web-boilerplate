{
  "name": "reactjs-web-boilerplate",
  "version": "1.0.6",
  "description": "Boilerplate for react based isomorphic application",
  "private": true,
  "repository": "git+ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/reactjs-web-boilerplate",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "agentkeepalive": "^2.0.5",
    "bluebird": "^3.3.4",
    "classnames": "^2.2.3",
    "eslint-config-360fy": "https://private-npm-releases.s3.amazonaws.com/eslint-config-360fy-1.0.1.tgz",
    "events": "^1.1.0",
    "immutable": "^3.7.6",
    "jquery": "^2.2.1",
    "js-yaml": "^3.5.4",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1",
    "rest": "^1.3.1",
    "velocity-animate": "^1.2.3",
    "velocity-react": "^1.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-node5": "^11.0.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-360fy": "https://private-npm-releases.s3.amazonaws.com/eslint-config-360fy-1.0.1.tgz",
    "eslint-plugin-react": "^4.2.3"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
